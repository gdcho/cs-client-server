<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
<servlet>
    <servlet-name>TriviaManagement</servlet-name>
    <servlet-class>Trivia</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>TriviaManagement</servlet-name>
    <url-pattern>/trivia/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Accounts</servlet-name>
    <servlet-class>Account</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Accounts</servlet-name>
    <url-pattern>/account/*</url-pattern>
  </servlet-mapping>
</web-app>
